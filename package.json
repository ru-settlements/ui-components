{
  "name": "@ru-settlements/ui-components",
  "version": "0.2.10",
  "description": "Ui components for settlements",
  "main": "dist/ui.umd.cjs",
  "module": "dist/ui.js",
  "type": "module",
  "types": "./dist/ui.d.ts",
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/ui.js",
      "require": "./dist/ui.js"
    },
    "./dist/lib.css": "./dist/lib.css"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ru-settlements/ui-components.git"
  },
  "author": "martiiian <jkramorevx@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "yarn run lint --fix",
    "storybook": "storybook dev -p 6006",
    "storybook:publish": "chromatic --project-token=$(grep PROJECT_TOKEN .env.local | cut -d '=' -f2)",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@mdi/js": "^7.2.96",
    "mdi-vue": "^3.0.13",
    "moment": "^2.29.4",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@storybook/addon-actions": "^7.0.5",
    "@storybook/addon-essentials": "^7.0.5",
    "@storybook/addon-interactions": "^7.0.5",
    "@storybook/addon-links": "^7.0.5",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/vue3": "^7.0.5",
    "@storybook/vue3-vite": "^7.0.5",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "babel-loader": "^8.3.0",
    "chromatic": "^6.17.3",
    "eslint": "^8.37.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-vue": "^9.10.0",
    "sass": "^1.60.0",
    "storybook": "^7.0.5",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.3.0",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^1.2.0"
  }
}
